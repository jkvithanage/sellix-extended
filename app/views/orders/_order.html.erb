<tr>
  <td class="whitespace-nowrap py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:pl-6"><%= Time.at(order.updated_at).to_date %></td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500"><%= order.uniqid %></td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500"><%= order.customer_email %></td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500"><%= Coupon.find_by(uniqid: order.coupon_uniqid).nil? ? "-" : Coupon.find_by(uniqid: order.coupon_uniqid).code %></td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500"><%= order.gateway %></td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 text-right"><%= number_with_precision(order.discount, precision: 2) %></td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500 text-right"><%= number_with_precision(order.total, precision: 2) %></td>
</tr>
